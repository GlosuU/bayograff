{
	"name": "bayograff",
	"version": "1.1.2",
	"description": "online biographies, journals and chronologies",
	"private": true,
	"main": "index.js",
	"scripts": {
		"clean": "rm -rf node_modules",
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "npm-run-all --parallel server-dev client-dev",
		"server-dev": "NODE_ENV=development nodemon server/index.js",
		"start": "node server/index.js",
		"client-dev": "vue-cli-service serve client/src/main.js",
		"client-build": "vue-cli-service build client/src/main.js",
		"client-lint": "vue-cli-service lint client/src/main.js",
		"reinstall": "npm run clean && npm install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GlosuU/bayograff.git"
	},
	"author": "GlosuU",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/GlosuU/bayograff/issues"
	},
	"homepage": "https://github.com/GlosuU/bayograff#readme",
	"dependencies": {
		"@auth0/auth0-spa-js": "^1.12.1",
		"@vue/cli-service": "~4.5.0",
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-plugin-router": "^4.5.4",
		"axios": "^0.21.2",
		"bootstrap": "^4.5.2",
		"bootstrap-social": "^5.1.1",
		"bootstrap-vue": "^2.16.0",
		"core-js": "^3.6.5",
		"cors": "^2.8.5",
		"express": "^4.17.3",
		"express-jwt": "^6.0.0",
		"jwks-rsa": "^1.10.1",
		"mongoose": "^5.13.15",
		"morgan": "^1.10.0",
		"node-forge": ">=1.3.0",
		"npm-run-all": "^4.1.5",
		"tiptap": "^1.30.0",
		"tiptap-extensions": "^1.33.2",
		"vue": "^2.6.12",
		"vue-confirm-dialog": "1.0.2",
		"vue-loading-spinner": "^1.0.11",
		"vue-router": "^3.2.0",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"babel-eslint": "^10.1.0",
		"vue-template-compiler": "^2.6.12"
	},
	"devDependencies": {
		"dotenv": "^8.2.0",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.1"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"engines": {
		"node": "12.x"
	}
}
